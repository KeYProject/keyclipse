<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

	
	<xs:complexType name="generic_spec">
		<xs:attribute name="keyword" type="xs:string" use="required"/>
	</xs:complexType>
	
	<xs:complexType name="keyword">
		<xs:attribute name="name" type="xs:string" use="required"/>
	</xs:complexType>
	
	<xs:complexType name="behavior_spec">
		<xs:sequence>
			<xs:element name="keyword" type="keyword" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>			
	</xs:complexType>
	
	<xs:complexType name="profile">
		<xs:sequence>
			<xs:element name="behavior_spec" type="behavior_spec" maxOccurs="unbounded"/>
			<xs:element name="generic_spec" type="generic_spec" maxOccurs="unbounded"/>
		</xs:sequence>
		<xs:attribute name="name" type="xs:string" use="required"/>
		<xs:attribute name="id" type="xs:string" use="required"/>	
	</xs:complexType>
	
	<xs:element name="profile" type="profile"/>
	
</xs:schema>