<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.key_project.key4eclipse.resources.ui.KeYProjectWizard"
         name="KeYProjectWizard"
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.key_project.key4eclipse.resources.ui.KeYCategory"
            class="org.key_project.key4eclipse.resources.ui.wizard.KeYProjectWizard"
            icon="icons/logo16.gif"
            id="org.key_project.key4eclipse.resources.ui.keyprojectwizard"
            name="KeY Project"
            project="true">
         <description>
            Create a KeY project
         </description>
      </wizard>
      <category
            id="org.key_project.key4eclipse.resources.ui.KeYCategory"
            name="KeY">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.key_project.key4eclipse.resources.ui.convertToKeYProjectCommand"
               icon="icons/logo16.gif"
               label="Convert to KeY Project"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <and>
                        <instanceof
                              value="org.eclipse.core.resources.IProject">
                        </instanceof>
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.eclipse.jdt.core.javanature">
                        </test>
                        <not>
                           <test
                                 forcePluginActivation="true"
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.key_project.key4eclipse.resources.KeYProjectNature">
                           </test>
                        </not>
                     </and>
                     <and>
                        <instanceof
                              value="org.eclipse.jdt.core.IJavaProject">
                        </instanceof>
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <not>
                              <test
                                    forcePluginActivation="true"
                                    property="org.eclipse.core.resources.projectNature"
                                    value="org.key_project.key4eclipse.resources.KeYProjectNature">
                              </test>
                           </not>
                        </adapt>
                     </and>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildAllProofsCommand"
               icon="icons/logo16.gif"
               label="Build all proofs"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.eclipse.jdt.core.javanature">
                        </test>
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.key_project.key4eclipse.resources.KeYProjectNature">
                        </test>
                     </adapt>
                     <or>
                        <instanceof
                              value="org.eclipse.jdt.core.IJavaProject">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IPackageFragmentRoot">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IPackageFragment">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.ICompilationUnit">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IType">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IMethod">
                        </instanceof>
                        <test
                              forcePluginActivation="true"
                              property="org.key_project.key4eclipse.resources.ui.isProofFile">
                        </test>
                        <test
                              forcePluginActivation="true"
                              property="org.key_project.key4eclipse.resources.ui.isProofFolder">
                        </test>
                     </or>
                  </iterate>
                  <iterate>
                     <with
                           variable="activePartId">
                        <equals
                              value="org.eclipse.jdt.ui.CompilationUnitEditor">
                        </equals>
                     </with>
                     <with
                           variable="activeEditorInput">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <test
                                 forcePluginActivation="true"
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.eclipse.jdt.core.javanature">
                           </test>
                        </adapt>
                     </with>
                     <with
                           variable="selection">
                        <and>
                           <instanceof
                                 value="org.eclipse.jface.text.ITextSelection">
                           </instanceof>
                        </and>
                     </with>
                  </iterate>
               </or>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildOutdatedProofsCommand"
               icon="icons/logo16.gif"
               label="Build outdated proofs"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <or>
                  <iterate
                        ifEmpty="false"
                        operator="and">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.eclipse.jdt.core.javanature">
                        </test>
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.key_project.key4eclipse.resources.KeYProjectNature">
                        </test>
                     </adapt>
                     <or>
                        <instanceof
                              value="org.eclipse.jdt.core.IJavaProject">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IPackageFragmentRoot">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IPackageFragment">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.ICompilationUnit">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IType">
                        </instanceof>
                        <instanceof
                              value="org.eclipse.jdt.core.IMethod">
                        </instanceof>
                        <test
                              forcePluginActivation="true"
                              property="org.key_project.key4eclipse.resources.ui.isProofFile">
                        </test>
                        <test
                              forcePluginActivation="true"
                              property="org.key_project.key4eclipse.resources.ui.isProofFolder">
                        </test>
                     </or>
                  </iterate>
                  <iterate>
                     <with
                           variable="activePartId">
                        <equals
                              value="org.eclipse.jdt.ui.CompilationUnitEditor">
                        </equals>
                     </with>
                     <with
                           variable="activeEditorInput">
                        <adapt
                              type="org.eclipse.core.resources.IResource">
                           <test
                                 forcePluginActivation="true"
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.eclipse.jdt.core.javanature">
                           </test>
                        </adapt>
                     </with>
                     <with
                           variable="selection">
                        <and>
                           <instanceof
                                 value="org.eclipse.jface.text.ITextSelection">
                           </instanceof>
                        </and>
                     </with>
                  </iterate>
               </or>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.key_project.key4eclipse.resources.ui.category"
            name="KeY Resources">
      </category>
      <command
