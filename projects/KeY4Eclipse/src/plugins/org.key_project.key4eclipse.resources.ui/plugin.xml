<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         id="org.key_project.key4eclipse.resources.ui.KeYProjectWizard"
         name="KeYProjectWizard"
         point="org.eclipse.ui.newWizards">
      <wizard
            category="org.key_project.key4eclipse.resources.ui.KeYCategory"
            class="org.key_project.key4eclipse.resources.ui.wizard.KeYProjectWizard"
            icon="icons/logo16.gif"
            id="org.key_project.key4eclipse.resources.ui.keyprojectwizard"
            name="KeY Project"
            project="true">
         <description>
            Create a KeY project
         </description>
      </wizard>
      <category
            id="org.key_project.key4eclipse.resources.ui.KeYCategory"
            name="KeY">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            allPopups="true"
            locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <command
               commandId="org.key_project.key4eclipse.resources.ui.convertToKeYProjectCommand"
               icon="icons/logo16.gif"
               label="Convert to KeY Project"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <or>
                     <and>
                        <instanceof
                              value="org.eclipse.core.resources.IProject">
                        </instanceof>
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.eclipse.jdt.core.javanature">
                        </test>
                        <not>
                           <test
                                 forcePluginActivation="true"
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.key_project.key4eclipse.resources.KeYProjectNature">
                           </test>
                        </not>
                     </and>
                     <and>
                        <instanceof
                              value="org.eclipse.jdt.core.IJavaProject">
                        </instanceof>
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <not>
                              <test
                                    forcePluginActivation="true"
                                    property="org.eclipse.core.resources.projectNature"
                                    value="org.key_project.key4eclipse.resources.KeYProjectNature">
                              </test>
                           </not>
                        </adapt>
                     </and>
                  </or>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildKeYProjectCommand"
               icon="icons/logo16.gif"
               label="Build KeY Project"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="org.eclipse.jdt.core.IJavaProject">
                  </instanceof>
                  <adapt
                        type="org.eclipse.core.resources.IProject">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.key_project.key4eclipse.resources.KeYProjectNature">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildProofStructuredSelectionCommand"
               icon="icons/logo16.gif"
               label="Build Proof"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="org.eclipse.jdt.core.IMethod">
                  </instanceof>
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.jdt.core.javanature">
                     </test>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.key_project.key4eclipse.resources.KeYProjectNature">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildProofTextSelectionCommand"
               icon="icons/logo16.gif"
               label="Build Proof"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate>
                  <with
                        variable="activePartId">
                     <equals
                           value="org.eclipse.jdt.ui.CompilationUnitEditor">
                     </equals>
                  </with>
                  <with
                        variable="activeEditorInput">
                     <adapt
                           type="org.eclipse.core.resources.IResource">
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.eclipse.jdt.core.javanature">
                        </test>
                     </adapt>
                  </with>
                  <with
                        variable="selection">
                     <and>
                        <instanceof
                              value="org.eclipse.jface.text.ITextSelection">
                        </instanceof>
                     </and>
                  </with>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildJavaFileCommand"
               icon="icons/logo16.gif"
               label="Build Java File"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <instanceof
                        value="org.eclipse.jdt.core.ICompilationUnit">
                  </instanceof>
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.jdt.core.javanature">
                     </test>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.key_project.key4eclipse.resources.KeYProjectNature">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
         <command
               commandId="org.key_project.key4eclipse.resources.ui.buildProofFileCommand"
               icon="icons/logo16.gif"
               label="Build Proof File"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <iterate
                     ifEmpty="false"
                     operator="and">
                  <test
                        forcePluginActivation="true"
                        property="org.key_project.key4eclipse.resources.ui.isProofFile">
                  </test>
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.jdt.core.javanature">
                     </test>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.key_project.key4eclipse.resources.KeYProjectNature">
                     </test>
                  </adapt>
               </iterate>
            </visibleWhen>
         </command>
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            id="org.key_project.key4eclipse.resources.ui.category"
            name="KeY Resources">
      </category>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.convertToKeYProjectCommand"
            name="Convert to KeY Project.">
      </command>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.buildKeYProjectCommand"
            name="Build KeY Project">
      </command>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.buildProofStructuredSelectionCommand"
            name="Build Proof">
      </command>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.buildProofTextSelectionCommand"
            name="Build Proof">
      </command>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.buildJavaFileCommand"
            name="Build Java File">
      </command>
      <command
            categoryId="org.key_project.key4eclipse.resources.ui.category"
            id="org.key_project.key4eclipse.resources.ui.buildProofFileCommand"
            name="Build Proof File">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.commandImages">
      <image
            commandId="org.key_project.key4eclipse.resources.ui.convertToKeYProjectCommand"
            icon="icons/logo16.gif">
      </image>
      <image
            commandId="org.key_project.key4eclipse.resources.ui.buildKeYProjectCommand"
            icon="icons/logo16.gif">
      </image>
      <image
            commandId="org.key_project.key4eclipse.resources.ui.buildProofStructuredSelectionCommand"
            icon="icons/logo16.gif">
      </image>
      <image
            commandId="org.key_project.key4eclipse.resources.ui.buildJavaFileCommand"
            icon="icons/logo16.gif">
      </image>
      <image
            commandId="org.key_project.key4eclipse.resources.ui.buildProofFileCommand"
            icon="icons/logo16.gif">
      </image>
      <image
            commandId="org.key_project.key4eclipse.resources.ui.buildProofTextSelectionCommand"
            icon="icons/logo16.gif">
      </image>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.ConvertJavaToKeYProjectHandler"
            commandId="org.key_project.key4eclipse.resources.ui.convertToKeYProjectCommand">
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <or>
                  <and>
                     <instanceof
                           value="org.eclipse.core.resources.IProject">
                     </instanceof>
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.jdt.core.javanature">
                     </test>
                     <not>
                        <test
                              forcePluginActivation="true"
                              property="org.eclipse.core.resources.projectNature"
                              value="org.key_project.key4eclipse.resources.KeYProjectNature">
                        </test>
                     </not>
                  </and>
                  <and>
                     <instanceof
                           value="org.eclipse.jdt.core.IJavaProject">
                     </instanceof>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                        <not>
                           <test
                                 forcePluginActivation="true"
                                 property="org.eclipse.core.resources.projectNature"
                                 value="org.key_project.key4eclipse.resources.KeYProjectNature">
                           </test>
                        </not>
                     </adapt>
                  </and>
               </or>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.BuildKeYProjectHandler"
            commandId="org.key_project.key4eclipse.resources.ui.buildKeYProjectCommand">
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="org.eclipse.jdt.core.IJavaProject">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IProject">
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.key_project.key4eclipse.resources.KeYProjectNature">
                  </test>
               </adapt>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.BuildProofStructuredSelectionHandler"
            commandId="org.key_project.key4eclipse.resources.ui.buildProofStructuredSelectionCommand">
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="org.eclipse.jdt.core.IMethod">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IResource">
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.eclipse.jdt.core.javanature">
                  </test>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.key_project.key4eclipse.resources.KeYProjectNature">
                  </test>
               </adapt>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.BuildProofTextSelectionHandler"
            commandId="org.key_project.key4eclipse.resources.ui.buildProofTextSelectionCommand">
         <enabledWhen>
            <iterate>
               <with
                     variable="activePartId">
                  <equals
                        value="org.eclipse.jdt.ui.CompilationUnitEditor">
                  </equals>
               </with>
               <with
                     variable="activeEditorInput">
                  <adapt
                        type="org.eclipse.core.resources.IResource">
                     <test
                           forcePluginActivation="true"
                           property="org.eclipse.core.resources.projectNature"
                           value="org.eclipse.jdt.core.javanature">
                     </test>
                  </adapt>
               </with>
               <test
                     forcePluginActivation="true"
                     property="org.key_project.key4eclipse.resources.ui.isMethodSelected">
               </test>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.BuildFileHandler"
            commandId="org.key_project.key4eclipse.resources.ui.buildJavaFileCommand">
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <instanceof
                     value="org.eclipse.jdt.core.ICompilationUnit">
               </instanceof>
               <adapt
                     type="org.eclipse.core.resources.IResource">
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.eclipse.jdt.core.javanature">
                  </test>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.key_project.key4eclipse.resources.KeYProjectNature">
                  </test>
               </adapt>
            </iterate>
         </enabledWhen>
      </handler>
      <handler
            class="org.key_project.key4eclipse.resources.ui.handlers.BuildFileHandler"
            commandId="org.key_project.key4eclipse.resources.ui.buildProofFileCommand">
         <enabledWhen>
            <iterate
                  ifEmpty="false"
                  operator="and">
               <test
                     forcePluginActivation="true"
                     property="org.key_project.key4eclipse.resources.ui.isProofFile">
               </test>
               <adapt
                     type="org.eclipse.core.resources.IResource">
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.eclipse.jdt.core.javanature">
                  </test>
                  <test
                        forcePluginActivation="true"
                        property="org.eclipse.core.resources.projectNature"
                        value="org.key_project.key4eclipse.resources.KeYProjectNature">
                  </test>
               </adapt>
            </iterate>
         </enabledWhen>
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.propertyPages">
      <page
            category="org.key_project.key4eclipse.common.ui.keyProjectProperties"
            class="org.key_project.key4eclipse.resources.ui.propertypages.ProofManagementPropertyPage"
            id="org.key_project.key4eclipse.resources.ui.proofManagementPropertyPage"
            name="KeY Proof Management">
         <enabledWhen>
            <adapt
                  type="org.eclipse.core.resources.IProject">
               <test
                     forcePluginActivation="true"
                     property="org.eclipse.core.resources.projectNature"
                     value="org.key_project.key4eclipse.resources.KeYProjectNature">
               </test>
            </adapt>
         </enabledWhen>
      </page>
   </extension>
      <extension
         point="org.eclipse.ui.editors">
      <editor
            class="org.eclipse.ui.editors.text.TextEditor"
            contributorClass="org.eclipse.ui.editors.text.TextEditorActionContributor"
            default="true"
            extensions="proofmeta"
            icon="icons/proofmetafile16.png"
            id="org.key_project.key4eclipse.common.ui.keyTextEditor"
            name="Proof Meta Text Editor">
      </editor>
   </extension>
      <extension
            point="org.eclipse.jdt.ui.javaElementFilters">
         <filter
               class="org.key_project.key4eclipse.resources.ui.propertypages.ProofmetaFilter"
               enabled="true"
               id="org.key_project.key4eclipse.resources.propertypages.proofmetafilter.packageexplorer"
               name="Proofmeta Filter"
               targetId="org.eclipse.jdt.ui.PackageExplorer">
         </filter>
      </extension>
      <extension
            point="org.eclipse.ui.navigator.navigatorContent">
         <commonFilter
               activeByDefault="true"
               class="org.key_project.key4eclipse.resources.ui.propertypages.ProofmetaFilter"
               id="org.key_project.key4eclipse.resources.propertypages.proofmetafilter.projectexplorer"
               name="Proofmeta Filter"
               visibleInUI="true">
         </commonFilter>
      </extension>
      <extension
            point="org.eclipse.ui.navigator.viewer">
         <viewerContentBinding
               viewerId="org.eclipse.ui.navigator.ProjectExplorer">
            <includes>
               <contentExtension
                     pattern="org.key_project.key4eclipse.resources.propertypages.proofmetafilter.projectexplorer">
               </contentExtension>
            </includes>
         </viewerContentBinding>
      </extension>
      <extension
            point="org.eclipse.core.expressions.propertyTesters">
         <propertyTester
               class="org.key_project.key4eclipse.resources.ui.expression.IsProofFilePropertyTester"
               id="org.key_project.key4eclipse.resources.ui.propertyTester.isProofFile"
               namespace="org.key_project.key4eclipse.resources.ui"
               properties="isProofFile"
               type="java.lang.Object">
         </propertyTester>
         <propertyTester
               class="org.key_project.key4eclipse.resources.ui.expression.IsMethodSelectedTester"
               id="org.key_project.key4eclipse.resources.ui.propertyTester.isMethodSelected"
               namespace="org.key_project.key4eclipse.resources.ui"
               properties="isMethodSelected"
               type="java.lang.Object">
         </propertyTester>
      </extension>
</plugin>
