\problem {
\forall Heap h; \forall LocSet s; \forall Heap h2; \forall Object o;
   (
   !o = null & !boolean::select(anon(h, s, h2), o, java.lang.Object::<created>) = TRUE
   
   <->
      !o = null
      & !boolean::select(h, o, java.lang.Object::<created>) = TRUE
       & !boolean::select(h2, o, java.lang.Object::<created>) = TRUE

   )
}