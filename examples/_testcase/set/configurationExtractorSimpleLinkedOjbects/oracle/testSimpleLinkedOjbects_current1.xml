<?xml version="1.0" encoding="UTF-8"?>
<model>
   <equivalenceClass representativeTerm="x.next">
      <term term="x.next"/>
      <term term="x.next.next"/>
   </equivalenceClass>
   <state name="return x.value+x.next.value+x.next.next.value;">
      <association programVariable="x" target="o1"/>
   </state>
   <object xml:id="o1" name="x" type="SimpleLinkedOjbects">
      <value programVariable="value" value="1" type="int"/>
      <association programVariable="next" target="o2"/>
   </object>
   <object xml:id="o2" name="x.next" type="SimpleLinkedOjbects">
      <value programVariable="value" value="3" type="int"/>
      <association programVariable="next" target="o2"/>
   </object>
</model>
