<?xml version="1.0" encoding="UTF-8"?>
<model>
   <state name="One Step Simplification resulting in return x.value+x.next.value;">
      <association programVariable="x" target="o1"/>
   </state>
   <object xml:id="o1" name="x" type="SimpleLinkedOjbectsDeletion">
      <association programVariable="next" target="o2"/>
   </object>
   <object xml:id="o2" name="x.next" type="SimpleLinkedOjbectsDeletion">
      <association programVariable="next" target="o3"/>
   </object>
   <object xml:id="o3" name="x.next.next" type="SimpleLinkedOjbectsDeletion">
   </object>
</model>
