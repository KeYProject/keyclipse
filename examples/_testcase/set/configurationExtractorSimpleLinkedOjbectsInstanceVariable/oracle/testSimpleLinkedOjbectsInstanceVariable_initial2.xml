<?xml version="1.0" encoding="UTF-8"?>
<model>
   <equivalenceClass representativeTerm="self.next">
      <term term="self.next"/>
      <term term="self.next.next"/>
   </equivalenceClass>
   <state name="One Step Simplification resulting in return this.value+this.next.value+this.next.next.value;">
      <association programVariable="self" target="o1"/>
   </state>
   <object xml:id="o1" name="self" type="SimpleLinkedOjbectsInstanceVariable">
      <association programVariable="next" target="o2"/>
   </object>
   <object xml:id="o2" name="self.next" type="SimpleLinkedOjbectsInstanceVariable">
      <association programVariable="next" target="o2"/>
   </object>
</model>
