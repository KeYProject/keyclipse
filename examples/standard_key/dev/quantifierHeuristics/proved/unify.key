//A simplify sub goal from /example/pred_log/tptp/puz/puz001p1-eq.key

\sorts {
  S;
}

\functions {
  S agatha;
  S butler;
}

\predicates {
  hates(S, S);
}


\problem {
 hates(butler, butler)&
 \forall S w4;  (w4 = butler | hates(agatha, w4)) &
 \forall S w5; (!hates(agatha,w5)|hates(butler,w5)) &
 \forall S w7;  \exists S w8;  !hates(w7, w8)
 ->false
 
}
