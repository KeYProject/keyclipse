\functions {
	int abs(int);
	int n; int q; int d;
}


\problem {
	n >= int_MIN & n <= int_MAX & d >= int_MIN & d <= int_MAX & d != 0
->
	jmod(n,d) >= int_MIN
	&
	jmod(n,d) <= int_MAX
	&
	addJint(mulJint(divJint(n,d),d), jmod(n,d)) = n
}
