<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.key_project.jmlediting.core.profiles">
      <profile
            class="org.key_project.jmlediting.profile.jmlref.JMLReferenceProfileAE">
      </profile>
      <profile
            class="org.key_project.jmlediting.profile.jmlref.JMLReferenceProfileBE">
      </profile>
      <profile
            class="org.key_project.jmlediting.profile.jmlref.JMLReferenceProfile">
      </profile>
   </extension>
        <extension
         point="org.eclipse.ltk.core.refactoring.renameParticipants">
      <renameParticipant
            class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLRenameParticipantFields"
            id="org.key_project.jmlediting.profile.jmlref.renameJMLParticipantFields"
            name="%renameJMLParticipantFields">
            <enablement>
              <with variable="affectedNatures">
                <iterate operator="or">
                  <equals value="org.eclipse.jdt.core.javanature"/>
                </iterate>
              </with>
              <with variable="element">
                <instanceof value="org.eclipse.jdt.core.IField"/>
              </with>
            </enablement>
      </renameParticipant>
   </extension>
     <extension
   point="org.eclipse.ltk.core.refactoring.renameParticipants">
<renameParticipant
      class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLRenameParticipantParameters"
      id="org.key_project.jmlediting.profile.jmlref.renameJMLParticipantParameters"
      name="%renameJMLParticipantParameters">
      <enablement>
        <with variable="affectedNatures">
          <iterate operator="or">
            <equals value="org.eclipse.jdt.core.javanature"/>
          </iterate>
        </with>
        <with variable="element">
          <instanceof value="org.eclipse.jdt.core.ILocalVariable"/>
        </with>
      </enablement>
</renameParticipant>
</extension>
     <extension
   point="org.eclipse.ltk.core.refactoring.renameParticipants">
<renameParticipant
      class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLRenameParticipantClass"
      id="org.key_project.jmlediting.profile.jmlref.renameJMLParticipantClass"
      name="%renameJMLParticipantClass">
      <enablement>
        <with variable="affectedNatures">
          <iterate operator="or">
            <equals value="org.eclipse.jdt.core.javanature"/>
          </iterate>
        </with>
        <with variable="element">
          <instanceof value="org.eclipse.jdt.core.IType"/>
        </with>
      </enablement>
</renameParticipant>
</extension>
     <extension
point="org.eclipse.ltk.core.refactoring.moveParticipants">
<moveParticipant
     id="org.eclipse.jdt.junit.moveClassParticipant"
     name="JML Move Participant Class"
class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLMoveParticipantClass">
     <enablement>
       <with variable="affectedNatures">
         <iterate operator="or">
           <equals value="org.eclipse.jdt.core.javanature"/>
         </iterate>
       </with>
       <with variable="element">
         <instanceof value="org.eclipse.jdt.core.IType"/>
       </with>
     </enablement>
</moveParticipant>
         </extension>
     <extension
           point="org.eclipse.ltk.core.refactoring.moveParticipants">
        <moveParticipant
              class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLMoveParticipantSMethod"
              id="org.key_project.jmlediting.profile.jmlref.moveSMethodParticipant"
              name="JML Move Participant Static Methods">
           <enablement>
           <with variable="affectedNatures">
         <iterate operator="or">
           <equals value="org.eclipse.jdt.core.javanature"/>
         </iterate>
       </with>
       <with variable="element">
         <instanceof value="org.eclipse.jdt.core.IMethod"/>
       </with>
           </enablement>
        </moveParticipant>
     </extension>
     <extension
           point="org.eclipse.ltk.core.refactoring.moveParticipants">
        <moveParticipant
              class="org.key_project.jmlediting.profile.jmlref.refactoring.JMLMoveParticipantSFields"
              id="org.key_project.jmlediting.profile.jmlref.moveSFieldParticipant1"
              name="JML Move Participant Static Fields">
           <enablement>
           <with variable="affectedNatures">
                <iterate operator="or">
                  <equals value="org.eclipse.jdt.core.javanature"/>
                </iterate>
              </with>
              <with variable="element">
                <instanceof value="org.eclipse.jdt.core.IField"/>
              </with>
              </enablement>
        </moveParticipant>
     </extension>
</plugin>
